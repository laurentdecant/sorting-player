    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "gh-pages": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.3",
    "parcel-bundler": "^1.12.4"
  },
  "browserslist": [
    "last 1 Chrome versions"
  ]
}

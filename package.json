{
  "name": "parcel-sandbox",
  "version": "1.0.0",
  "description": "Simple Parcel Sandbox",
  "main": "index.html",
  "homepage": "https://laurentdecant.github.io/sorting-visualizer",
  "scripts": {
    "start": "parcel index.html --open",
